<Window x:Class="ConvertNote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConvertNote"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="ConvertNote" Height="400" Width="600" Foreground="White" Background="#FF262525" Icon="Icon.ico">
    <Grid Margin="0,0,0,-6" Height="380.04">
        <StackPanel>
            <Menu>
                <MenuItem Header="Fichier">
                    <MenuItem Header="Quitter" x:Name="CLOSE" Click="CLOSE_Click"/>
                </MenuItem>
                <MenuItem Header="A propos">
                    <MenuItem Header="Informations" x:Name="INFOS" Click="INFOS_Click"/>
                </MenuItem>
            </Menu>
        </StackPanel>
        <Frame x:Name="frame1" Margin="0,19,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
